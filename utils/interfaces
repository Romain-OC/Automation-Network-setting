#/etc/network/interfaces

source /etc/network/interfaces.d/*

auto lo
iface lo inet loopback

#the primary network interface
#allow-hotplug eth0
#iface eth0 inet dhcp
	#address eth0
	#netmask eth0

#allow-hotplug eth1
#iface eth1 inet dhcp
	#address eth1
	#netmask eth1

#allow-hotplug eth2
#iface eth2 inet dhcp
	#address eth2
	#netmask eth2

